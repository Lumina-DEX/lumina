{
	"compilerOptions": {
		"allowJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"types": ["vite/client"],
		"noEmit": true
	},
	"include": ["src", "vite.config.ts", "tailwind.config.js", "postcss.config.js", "index.html"]
}
