{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "dprint.dprint",
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always"
    }
  },
  "dprint.path": "./node_modules/.bin/dprint"
}
