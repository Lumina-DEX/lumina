{
	"servers": {
		"moon": {
			"type": "stdio",
			"command": "moon",
			"args": ["mcp"],
			"cwd": "${workspaceFolder}"
		},
		"playwright": {
			"command": "npx",
			"args": ["@playwright/mcp@latest"]
		},
		"cloudflare-documentation": {
			"command": "npx",
			"args": ["mcp-remote", "https://docs.mcp.cloudflare.com/sse"]
		}
	}
}
